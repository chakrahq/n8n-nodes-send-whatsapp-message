{
    "name": "@chakrahq/n8n-nodes-send-whatsapp-message",
    "version": "0.1.0",
    "description": "A chakrahq n8n node for sending whatsapp messages",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "ChakraHQ",
        "email": "engineering@chakrahq.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chakrahq/n8n-nodes-send-whatsapp-message.git"
    },
   "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/GithubIssuesApi.credentials.js",
            "dist/credentials/GithubIssuesOAuth2Api.credentials.js",
            "dist/credentials/ChakraWhatsappApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/GithubIssues/GithubIssues.node.js",
            "dist/nodes/SendWhatsappMessage/SendWhatsappMessage.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "private": false
}
